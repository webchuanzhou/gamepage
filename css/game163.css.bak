body{
	width:100%;
	min-height:930px;
	height:auto;
	background:#000;
}
*{
	padding:0;
	margin:0;
	list-style:none;
	font-family:"微软雅黑";
}
a,a:hover{
	text-decoration:none;
	outline:0;
	-moz-outline:0;
}
/*footer样式*/
#footer {
	padding:20px 0;
	background:#272a2c;
	min-width:1260px;
	color:#fff;
}
#footer a{
	color:#fff;
}
#footer a:hover{
	text-decoration:underline;
}
#footer p{
	width:765px;
	text-align:left;
	margin:0 auto;
	padding:15px 0 0;
	font-size:12px;
	font-family:"宋体";
	line-height:20px;
	display:block;
	position:relative;
}
#footer p .ft_1{
	vertical-align:top;
	top:4px;
	display:inline-block;
	position:relative;
}
#footer p .ft_1 a{
	height:31px;
	display:inline-block;
	cursor:pointer;
	padding-right:15px;
}
#footer p .ft_1 .ft_11{
	width:118px;
	padding-right:0;
	background:url("../images/netease.2.png") no-repeat;
}
#footer p .ft_1 .ft_12{
	width:134px;
	background:url("../images/nie.2.png") no-repeat;
}
#footer p .ft_1 .ft_13{
	width:31px;
	background:url("../images/wen.png") no-repeat;
}
#footer p .ft_2{
	text-align:left;
	display:inline-block;
	padding-left:6px;
}
#footer p .ft_2 .ft_21,#footer p .ft_2 .ft_21 a{
	color:#e23a3f;
}
#footer p .ft_3{
	display:inline-block;
	padding-top:10px;
}
/*wrep样式*/
.wrap{
	position:relative;
	width:100%;
	min-width:1280px;
	max-width:1920px;
	margin:0 auto;
}
.topbar{
	position:absolute;
	width:100%;
	background:#fff;
	opacity:.95;
	z-index:9999;
	min-width:1280px;
}
.topbar .topbox{
	position:relative;
	width:1260px;
	margin:0 auto;
	height:100px;
	overflow:visible;
}
.topbar .f-cb:after{
	display:block;
	clear:both;
	visibility:hidden;
	height:0;
	overflow:hidden;
	content:".";
}
.topbar .toplogo{
	position:relative;
	float:left;
	width:191px;
	height:58px;
	background:url("../images/logo1_1c91104.png") no-repeat;
	top:21px;
	display:block;
}
.topbar .toplist{
	position:relative;
	height:100px;
	line-height:100px;
	margin-left:270px;
	color:#333;
}
.topbar .toplist a{
	position:relative;
	float:left;
	margin:0 25px;
	font-size:16px;
	color:#888;
	height:100px;
	line-height:100px;
}
.topbar .toplist a:hover{
	color:#000;
}
.topbar .toplist a:hover i{
	width:100%;
}
.topbar .toplist a i{
	position:absolute;
	width:0;
	height:5px;
	background:#cf0f32;
	display:block;
	left:0;
	top:0;
	-webkit-transition:all .2s ease-out;
	-moz-transition:all .2s ease-out;
	-ms-transition:all .2s ease-out;
	-o-transition:all .2s ease-out;
	transition:all .2s ease-out;
}
.topbar .toplist span{
	float:left;
	height:100px;
	line-height:100px;
	width:4px;
}
.topOpen{
	position:absolute;
	top:0;
	right:0;
	width:120px;
	height:120px;
	background:#cf1132;
	z-index:9999;
	-webkit-transition: all .3s ease-in;
	-moz-transition: all .3s ease-in;
	-ms-transition: all .3s ease-in;
	-o-transition: all .3s ease-in;
	transition: all .3s ease-in;
}
.topOpen i,.topOpen.open i,.closeNav i,.closeNav:hover i,.icon-N,.icon-H{
	background-size:68px 62px;
	background-image:url("../images/topBar_1_z_e7632fe.png");
}
.topOpen i{
	position:relative;
	width:34px;
	height:34px;
	display:block;
	margin:0 auto;
	top:43px;
	background-repeat: no-repeat;
	background-position:0px 0px;
}
.topOpen.open{
	background-color:#282b2d;
	height:100px;
}
.topOpen.open i{
	background-position:-34px 0px;
	background-repeat: no-repeat;
}
.topNav{
	position:absolute;
	width:100%;
	padding-bottom:60px;
	top:100px;
	left:0;
	background:#282b2d;
	z-index:10000;
	display:none;
}
.topNav p{
	position:relative;
	text-indent:25px;
	font-size:20px;
	height:20px;
	line-height:20px;
	color:#fff;
	border-left:4px solid #cf1132;
	margin:0 40px;
	margin-top:60px;
}
.topNav span{
	float:right;
	color:#f2ebeb;
	font-size:12px;
}
.icon-H,.icon-N{
	width:11px;
	height:11px;
	display:inline-block;
	margin:0 5px;
}
.icon-H{
	background-position:-56px -48px;
	background-repeat:no-repeat;
}
.icon-N{
	background-position:-56px -34px;
	background-repeat:no-repeat;
}
.nav-li-box{
	margin-top:40px;
	float:left;
	border-left:1px solid #454545;
}
.nav-li-box.nav-li-box1{
	border-left:0;
	width:177px;
}
.nav-li-box.nav-li-box2{

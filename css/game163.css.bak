}
.closeNav{
	position:absolute;
	bottom:5px;
	left:47%;
	padding:10px 0;
}
.closeNav i{
	background-position:0 -34px;
	background-repeat:no-repeat;
	width:56px;
	height:14px;
	display:block;
}
.closeNav:hover i{
	background-position:0 -48px;
	background-repeat:no-repeat;
}
